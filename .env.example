# Lithox Epoxy App - Environment Variables
# Copy this file to .env and fill in your actual values

# SendGrid Email Service (for production email notifications)
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Admin Contact
ADMIN_EMAIL=admin@lithox.com

# Firebase Configuration (Optional - can be configured via firebase_options.dart)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_APP_ID=your_firebase_app_id

# Database Configuration (if using custom backend)
DATABASE_URL=your_database_url_here

# API Keys for third-party services
MAPS_API_KEY=your_google_maps_api_key
RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# App Configuration
APP_VERSION=1.0.0
DEBUG_MODE=true
